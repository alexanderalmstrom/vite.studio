{
  "name": "vite.studio",
  "version": "0.0.1",
  "scripts": {
    "dev": "node server",
    "build": "run-s build:client build:server",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --outDir dist/server --ssr src/entry/server.tsx ",
    "preview": "vite preview",
    "format": "prettier **/*.{tsx,ts,scss} --write"
  },
  "dependencies": {
    "express": "^4.17.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.0.7",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}